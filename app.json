{
  "name": "Ogaalkoob",
  "displayName": "Ogaalkoob",
  "expo": {
    "name": "Ogaalkoob",
    "slug": "Ogaalkoob",
    "scheme": "ogaalkoob",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/playstore.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E1E1E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "jsEngine": "hermes",
    "assetBundlePatterns": ["**/*"],
    "android": {
      "icon": "./assets/images/playstore.png",
      "package": "com.ogaalkoob.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/playstore.png",
        "backgroundImage": "./assets/images/playstore.png"
      },
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#1E1E1E"
      }
    },
    "ios": {
      "icon": "./assets/images/appstore.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.ogaalkoob.app",
      "splash": {
        "image": "./assets/images/splash.png",
        "tabletImage": "./assets/images/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#1E1E1E"
      },
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      }
    },
    "web": {
      "favicon": "./assets/images/app-icon-web-favicon.png",
      "splash": {
        "image": "./assets/images/splash-logo-web.png",
        "resizeMode": "contain",
        "backgroundColor": "#1E1E1E"
      },
      "bundler": "metro"
    },
    "plugins": [
      "@config-plugins/react-native-blob-util",
      "@config-plugins/react-native-pdf",

      "expo-localization",
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": false
          },
          "android": {
            "newArchEnabled": false
          }
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true
    }
  },
  "ignite": {
    "version": "9.0.0"
  }
}
